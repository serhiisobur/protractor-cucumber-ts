{
    "name": "protractor-cucumber-ts",
    "version": "0.0.1",
    "description": "Test project",
    "repository": "https://github.com/serhiisobur/protractor-cucumber-ts",
    "license": "MIT",
    "devDependencies": {
        "chai": "^4.2.0",
        "cucumber": "^5.1.0",
        "prettier": "1.16.4",
        "protractor": "^5.4.2",
        "protractor-cucumber-framework": "^6.1.1",
        "tslint": "^5.13.1",
        "typescript": "^3.3.3"
    },
    "scripts": {
        "format": "npx prettier --write \"{pageObjects,step_definitions}/**/*.ts\"",
        "format:check": "npx prettier --list-different \"{apps,libs}/**/*.ts\"",
        "wd-update": "node ./node_modules/protractor/bin/webdriver-manager update",
        "wd-start": "node ./node_modules/protractor/bin/webdriver-manager start",
